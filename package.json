{
  "name": "minimal-agent",
  "version": "0.1.0",
  "private": true,
  "engines": { 
    "node": "18.x"
  },
  "scripts": {
    "dev": "tsup src/agent/index.ts --out-dir api --format esm --dts --watch --onSuccess \"RUN_LOCAL_SERVER='true' node api/index.mjs\"",
    "build": "tsup src/agent/index.ts --out-dir api --format esm --dts",
    "start": "RUN_LOCAL_SERVER='true' node api/index.mjs",
    "vercel-build": "tsup src/agent/index.ts --out-dir api --format esm --dts",
    "test": "vitest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "chart.js": "^4.4.2",
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "vitest": "^1.0.0",
    "typescript": "^5.3.3",
    "tsup": "^8.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/uuid": "^9.0.8"
  }
} 